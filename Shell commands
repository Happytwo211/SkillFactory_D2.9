from django.contrib.auth.models import User
from News_Portal_v2 import *
user_1 = User.objects.create_user('User_1')  
user_1.save()
user_2 = User.objects.create_user('User_2')
user_2.save()
author_1 = Author.objects.create(author_to_user = user_1, user_rating = 0.0)  
author_1.save()
author_2 = Author.objects.create(author_to_user = user_2, user_rating = 0.0)
author_2.save()
category_1 = Category.objects.create(category_name = 'Sport')
category_1.save()                                             
category_2 = Category.objects.create(category_name = 'World') 
category_2.save()                                             
category_3 = Category.objects.create(category_name = 'Science') 
category_3.save()                                               
category_4 = Category.objects.create(category_name = 'Culture') 
category_4.save()         
post_1 = Post.objects.create(author_to_post = author_1, choose = 'AR', post_title_name = 'Post 1 Title', post_body_text = 'This is Post 1 text', post_rating = 0.0)  
post_1.save()
post_2 = Post.objects.create(author_to_post = author_2, choose = 'AR', post_title_name = 'Post 2 Title', post_body_text = 'This is Post 2 text', post_rating = 0.0) 
post_2.save() 
post_3 = Post.objects.create(author_to_post = author_2, choose = 'NW', post_title_name = 'Post 3 Title', post_body_text = 'This is Post 3 text', post_rating = 0.0)                                                                                                                                                     
post_3.save()     
comment_1 = Comment.objects.create(comment_to_post = post_1, comment_to_user = user_1, comment_body = 'This is comment_1 to post_1', comment_rating = 0.0)  
comment_1.save()
comment_2 = Comment.objects.create(comment_to_post = post_2, comment_to_user = user_2, comment_body = 'This is comment_2 to post_2', comment_rating = 0.0) 
comment_2.save()  
comment_3 = Comment.objects.create(comment_to_post = post_3, comment_to_user = user_2, comment_body = 'This is comment_3 to post_3', comment_rating = 0.0) 
comment_3.save()
comment_4 = Comment.objects.create(comment_to_post = post_3, comment_to_user = user_1, comment_body = 'This is comment_4 to post_3', comment_rating = 0.0) 
comment_4.save()
